<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="SentientRecyclerView">
        <attr name="emptyView" format="reference"/>
        <attr name="swipeableView" format="reference"/>
        <attr name="allowSwipe" format="flags">
            <flag name="none" value="0x0"/>
            <flag name="left" value="0x4"/>
            <flag name="right" value="0x8"/>
        </attr>
        <attr name="leftUnderSwipeableView" format="reference"/>
        <attr name="rightUnderSwipeableView" format="reference"/>
    </declare-styleable>
    <declare-styleable name="RoundedImageView">
        <attr name="tooltip" format="string" />
    </declare-styleable>
</resources>
